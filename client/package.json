    "@types/react-router-dom": "^5.3.3",
    "@zakapp/shared": "file:../shared",
    "ajv": "^8.17.1",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "dexie": "^4.2.1",
    "framer-motion": "^12.23.26",
    "hijri-converter": "^1.1.1",
    "js-sha256": "^0.10.1",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.562.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.28.1",
    "react-window": "^2.2.1",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^3.2.1",
    "rxdb": "^16.21.1",
    "rxjs": "^7.8.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "uuid": "^13.0.0",
    "web-vitals": "^2.1.4",
    "zod": "^3.22.4"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite preview --host --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --config=lighthouserc.json",
    "lighthouse:desktop": "lighthouse http://localhost:3000 --preset=desktop --view",
    "lighthouse:mobile": "lighthouse http://localhost:3000 --preset=mobile --view",
    "lighthouse:pwa": "lighthouse http://localhost:3000 --only-categories=pwa --view"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.1",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/axe-core": "^2.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.11.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fake-indexeddb": "^6.2.5",
    "fast-check": "^4.5.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "vite": "^6.4.1",
    "vite-plugin-pwa": "^0.17.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^3.2.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@pages/(.*)$": "<rootDir>/src/pages/$1",
      "^@hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^@contexts/(.*)$": "<rootDir>/src/contexts/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@types/(.*)$": "<rootDir>/src/types/$1",
      "^@data/(.*)$": "<rootDir>/src/data/$1",
      "^@zakapp/shared$": "<rootDir>/../shared/src/index.ts",
      "^@zakapp/shared/(.*)$": "<rootDir>/../shared/src/$1",
      "^(.*)\\.js$": "$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!zod)"
    ]
  }
}
