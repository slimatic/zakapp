{
  "name": "@zakapp/shared",
  "version": "0.9.2",
  "description": "Shared types and utilities for zakapp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types/tracking": {
      "import": "./dist/types/tracking.js",
      "require": "./dist/types/tracking.js",
      "types": "./dist/types/tracking.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./types/*": {
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.js",
      "types": "./dist/types/*.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./constants/*": {
      "import": "./dist/constants/*.js",
      "require": "./dist/constants/*.js",
      "types": "./dist/constants/*.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist tsconfig.tsbuildinfo && tsc --build",
    "postbuild": "node ./scripts/add-js-extension.cjs",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "zod": "^3.22.4",
    "js-sha256": "^0.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ]
}